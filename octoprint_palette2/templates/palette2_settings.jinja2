<div>
  <h4>Palette 2 Settings</h4>
  <div class="checkbox theme-input-label">
    <label>
      <span>Auto Connect To Palette 2</span>
      <input class="autoconnect-input" type="checkbox" data-bind="checked: settings.plugins.palette2.autoconnect">
    </label>
    <label>
      <span>Display Palette 2 Setup Alerts</span>
      <input class="alert-input" type="checkbox" data-bind="checked: settings.plugins.palette2.palette2Alerts">
    </label>
    <!-- SKELLATORE -->
    <label>
      <span>Enable Ping/Pong display on Printer</span>
      <input class="showpingpong-input" type="checkbox" data-bind="checked: settings.plugins.palette2.showpingpongonprinter">
    </label>
    <label>
      <span>Enable Feed-rate control during Splice</span>
      <input class="feedratecontrol-input" type="checkbox" data-bind="checked: settings.plugins.palette2.feedratecontrol">
    </label>
    <label>
      <span>Normal Feed-rate speed Percent</span>
      <input class="feedratenormal-input" type="number" min="1" max="100" data-bind="value: settings.plugins.palette2.feedratenormalpct">
    </label>
    <label>
      <span>Splicing Feed-rate speed Percent</span>
      <input class="feedrateslow-input" type="number" min="1" max="100" data-bind="value: settings.plugins.palette2.feedrateslowpct">
    </label>
    <!-- /SKELLATORE -->
  </div>
</div>